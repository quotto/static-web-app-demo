{
    "trailingSlash": "auto",
    "routes":[
        {
            "route": "/index.html",
            "allowedRoles": ["authenticated"]
        },
        {
            "route": "/logout",
            "redirect": "/.auth/logout"
        }
    ],
    "responseOverrides": {
        "401": {
        "statusCode": 302,
        "redirect": "/login.html"
        }
    },
    "navigationFallback": {
        "rewrite": "/login.html"
    },
    "globalHeaders": {
        "Access-Control-Allow-Methods": "GET"
    }
}